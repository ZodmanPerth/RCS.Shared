<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net10.0-windows10.0.26100.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <RootNamespace>OKB</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Assets\ICO\Logo.ico" />
    <None Remove="Assets\JPG\Avatar.jpg" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Assets\ICO\Logo.ico">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Assets\JPG\Avatar.jpg">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>
  
</Project>
